{
    "name": "imnicy/support",
    "type": "library",
    "description": "From Illuminate Support package.",
    "license": "MIT",
    "homepage": "https://imnicy.com",
    "authors": [
        {
            "name": "bin",
            "email": "support@imnicy.com"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "autoload": {
        "psr-4": {
            "Nicy\\Support\\": ""
        },
        "files": [
            "helpers.php"
        ]
    },
    "suggest": {
      "ramsey/uuid": "Required to use Str::uuid() (^3.7)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
